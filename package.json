    "eslint-plugin-prettier": "^3.4.1",
    "final-form": "^4.20.9",
    "leaflet": "^1.8.0",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-apexcharts": "^1.4.0",
